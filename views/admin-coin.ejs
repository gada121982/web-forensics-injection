  <body>
    <!DOCTYPE html>
    <html lang="en">
      <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link rel="stylesheet" type="text/css" href="./css/style.css" />
        <link rel="stylesheet" type="text/css" href="./css/user.css" />
        <link rel="stylesheet" type="text/css" href="./css/admin.css" />
        <title>Foregsis</title>
      </head>
      <body>
        <div class="wrap">
          <header>
            <img src="./img/logo.png" alt="logo" />
            <div class="header-right">
              <div class="home">
                <a href="/">Home</a>
              </div>
              <div class="user-info">
                <div class="user">
                    <a href="./manage"> 
                        <img class="avt-img" src="./img/avt.jpg" alt="avt" />
                    </a>  
                  </form>
                </div>
                <div class="show-coin">
                  <h5>Coin</h5>
                  <h5>100 <span>$</span></h5>
                </div>
              </div>
            </div>
          </header>
          <main>
              <!-- side bar -->
              <div class="side-bar">
                <!--Link to manage order -->
                <div class="manage-coin">
                    <a href="./coin">Manage Coin</a>
                </div>
                <!--Link to manage coin -->
                <div class="manage-coin">
                  <a href="./order">Manage Order</a>
                </div>
              </div>
              <!-- content -->
              <div class="data-content">
                    <%
                    coinListNotActive.forEach(function(coin){%>
                      <div class="coin-ele">
                        <div class="id-coin">
                          <h4><%= coin.id %></h4>
                        </div>
                        <!-- seri number-->
                        <div class="seri-number">
                          <label for="seri-number">Seri :</label>
                          <h4 id="seri-number"><%= coin.seri %></h4>
                        </div>
                        <!-- PIN -->
                        <div class="pin-number">
                          <label>PIN :</label>
                          <div class="hide-p">Hidden</div>
                        </div>
                        <!--Active button-->
                        <a  class="active-coin" href="/activepin/<%= coin.id%>">
                          Active
                        </a>
                    </div>
                    <%})
                    %>
                   
              </div>
          </main>
          <footer>
            <div class="project-infor">
              <div class="source-code">
                <h4>Source Code</h4>
                <div class="git">
                  <img src="./img/giticon.png" alt="git-icon" />
                  <a
                    href="https://github.com/gada121982/web-forensics-injection"
                    target="_blank"
                    >https://github.com/gada121982/web-forensics-injection</a
                  >
                </div>
              </div>
              <div class="warn">
                <img src="./img/warn.png" alt="" />
                <h5>
                  This website contains an error sql injection , Lest's find it
                  :3
                </h5>
              </div>
            </div>
            <div class="infor-author">
              <h4>Author information</h4>
              <div class="link-infor">
                <a href="https://www.facebook.com/vinhhai4199" target="_blank">
                  Facebook
                </a>
                <a href="https://twitter.com/NguynVnhHi1" target="_blank">
                  Twitter
                </a>
                <a href="https://www.linkedin.com/in/vinhhai" target="_blank">
                  Linkin
                </a>
              </div>
            </div>
          </footer>
        </div>
      </body>
    </html>
  </body>
</html>
